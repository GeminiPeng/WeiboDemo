OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/YYKit" -isystem "${PODS_ROOT}/Headers/Public/YYText"
OTHER_LDFLAGS = $(inherited) -ObjC -l"YYKit" -l"YYText" -l"z" -framework "Accelerate" -framework "CoreFoundation" -framework "CoreGraphics" -framework "CoreText" -framework "ImageIO" -framework "MobileCoreServices" -framework "QuartzCore" -framework "Security" -framework "UIKit"
PODS_ROOT = ${SRCROOT}/Pods